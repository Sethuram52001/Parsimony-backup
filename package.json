{
  "name": "Parsimony",
  "version": "1.0.0",
  "description": "web-app to keep track of expenses",
  "main": "index.js",
  "repository": "git@github.com:Sethuram52001/Parsimony.git",
  "author": "Sethuram <sethuram52001@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development nodemon index",
    "test": "NODE_ENV=test jest",
    "execute-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "config": "^3.3.8",
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "lint-staged": "^13.0.3",
    "mongodb-memory-server": "^8.9.5",
    "prettier": "^2.7.1",
    "supertest": "^6.3.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint . --fix",
      "prettier --write ."
    ]
  },
  "dependencies": {
    "@jest-mock/express": "^2.0.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^6.5.2",
    "nodemon": "^2.0.19"
  }
}
